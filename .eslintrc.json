{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "semi": ["error", "always"],
      "eqeqeq": ["error", "always"],
      "no-unused-vars": ["error", { "args": "none" }],
      "quotes": ["error", "single"]
    }
}
